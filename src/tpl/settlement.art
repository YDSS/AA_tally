<div class="settlement">
    <dl class="pay-area row">
        {{each tracks track index}}
        <dt class="col-sm-3">{{#index}}</dt>
        <dd class="col-sm-9">{{track}}</dd>
        {{/each}}
    </dl>
    <dl class="data-area row">
        {{each others item index}}
        <dt class="col-sm-3">{{item.title}}</dt>
        <dd class="col-sm-9">{{item.num}}</dd>
        {{/each}}
    </dl>
</div>